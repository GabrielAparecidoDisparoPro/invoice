{
  "name": "@ativoscapital/invoice",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Gabriel Aparecido",
    "email": "gabriel.aparecido@disparopro.com.br"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "check": "biome check --write ./src",
    "lint": "pnpm check && pnpm lint:fix",
    "lint:pre-commit": "pnpm check && pnpm lint:ci",
    "lint:ci": "biome lint --error-on-warnings ./src",
    "lint:fix": "biome lint --write ./src",
    "test": "vitest",
    "test:ci": "vitest --run",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "sdk:gen": "rename tsconfig.json changeMeToTsconfigDotJson_ImATempFile.json && rimraf ./sdk/api && npx nestia sdk --project ./sdk/sdk.tsconfig.json || true && rename changeMeToTsconfigDotJson_ImATempFile.json tsconfig.json",
    "sdk:version": "cd ./sdk && pnpm change-versions && pnpm apply-versions",
    "sdk:release": "cd ./sdk && pnpm release:ci",
    "cti": "cti ./src -b --withoutbackup -w --withoutcomment -i --fileexcludes *.spec.ts",
    "prepare": "ts-patch install && typia patch && husky"
  },
  "dependencies": {
    "@ativoscapital/jedi.node.core": "latest",
    "@ativoscapital/jedi.node.cqrs": "latest",
    "@ativoscapital/jedi.node.mongo": "latest",
    "@ativoscapital/jedi.node.nestjs": "latest",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.0.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.1.17",
    "bullmq": "^5.17.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@ativoscapital/jedi.node.config": "latest",
    "@biomejs/biome": "1.8.3",
    "@nestia/benchmark": "^0.2.3",
    "@nestia/core": "^3.11.3",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.11.3",
    "@nestia/sdk": "^3.11.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/core": "^1.3.67",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@vitest/coverage-v8": "^1.1.2",
    "@vitest/ui": "^1.1.1",
    "create-ts-index": "^1.14.0",
    "husky": "^9.1.4",
    "mongodb-memory-server": "^8.13.0",
    "nestia": "^5.7.0",
    "rename-cli": "^6.2.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5",
    "typia": "^6.9.0",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.1.1"
  }
