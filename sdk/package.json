{
  "name": "@ativoscapital/invoice-sdk",
  "version": "0.0.1",
  "description": "SDK library generated by Nestia for invoice",
  "exports": {
    ".": "./dist/sdk/sdk.js"
  },
  "scripts": {
    "build": "rimraf tsconfig.tsbuildinfo && tsc -b ./sdk.tsconfig.json",
    "change-versions": "pnpm i && pnpm changeset",
    "apply-versions": "pnpm changeset version",
    "release": "changeset publish",
    "release:ci": "pnpm install && pnpm build && pnpm release"
  },
  "repository": {
    "url": "https://github.com/ativoscapital/invoice"
  },
  "license": "MIT",
  "dependencies": {
    "@nestia/fetcher": "^3.4.1",
    "@ativoscapital/jedi.node.mongo": "latest",
    "@ativoscapital/jedi.node.core": "latest",
    "@ativoscapital/jedi.node.nestjs": "latest",
    "@ativoscapital/jedi.node.cqrs": "latest"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.4.2",
    "ts-patch": "^3.1.0",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2"
  }
}
